@Library('demo-shared-lib@feature/jenkinslib') _

pipeline {
    agent any

    stages {
        stage('Example') {
            steps {
                script {
                    // Invoke the shared method to run a bash command
                    def result = mySharedLibrary.runShellCommand('echo "Hello, Shared Library!"')
                    
                    // Check the result
                    if (result == 0) {
                        echo 'Command executed successfully'
                    } else {
                        error 'Command failed'
                    }
                }
            }
        }
    }
}